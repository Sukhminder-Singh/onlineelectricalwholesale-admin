{
  "name": "tailadmin-react",
  "private": true,
  "version": "2.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "NODE_ENV=production tsc -b && vite build",
    "build:force": "tsc -b --noEmitOnError false && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:warn": "eslint . --max-warnings 0",
    "preview": "vite preview",
    "copy-themes": "cpx \"node_modules/primereact/resources/themes/**/*\" public/themes",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/socket.io-client": "^1.4.36",
    "apexcharts": "^4.1.0",
    "clsx": "^2.1.1",
    "flatpickr": "^4.6.13",
    "framer-motion": "^12.19.2",
    "lucide-react": "^0.525.0",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.6",
    "react": "^19.0.0",
    "react-apexcharts": "^1.7.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.6.3",
    "socket.io-client": "^4.8.1",
    "swiper": "^11.2.3",
    "tailwind-merge": "^3.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@tailwindcss/postcss": "^4.0.8",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "cpx": "^1.5.0",
    "eslint": "^9.19.0",
